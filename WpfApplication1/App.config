    <section name="oracle.manageddataaccess.client"
      type="OracleInternal.Common.ODPMSectionHandler, Oracle.ManagedDataAccess, Version=4.122.1.0, Culture=neutral, PublicKeyToken=89b483f429c47342"/>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
  </startup>
  <connectionStrings>
    <add name="Mysql" connectionString="server=192.168.0.2;uid=test;pwd=XXXXXXXXXXX;database=test" providerName="MySql.Data.MySqlClient"/>
    <add name="Postgres" connectionString="Server=192.168.0.63;Port=5432;User Id=test;Password=XXXXXXXXXXX;Database=test;" providerName="Npgsql"/>
    <add name="Mssql"
      connectionString="Server=192.168.0.2;Asynchronous Processing=true;MultipleActiveResultSets=True;Initial Catalog=test;User ID=chaos;Password=XXXXXXXXXXX;"
      providerName="System.Data.SqlClient"/>
    <add name="testEntities"
      connectionString="metadata=res://*/Model1.csdl|res://*/Model1.ssdl|res://*/Model1.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=chaos2;initial catalog=test;persist security info=True;user id=chaos;password=XXXXXXXXXXX;asynchronous processing=True;multipleactiveresultsets=True;App=EntityFramework&quot;"
      providerName="System.Data.EntityClient"/>
